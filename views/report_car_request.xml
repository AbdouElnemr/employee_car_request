<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <template id="report_car_request">
            <t t-call="web.html_container">
                <t t-foreach="docs" t-as="doc">
                    <t t-call="web.external_layout">
                        <h1>This Report Created By
                            <span class="text-danger">Abdelhameed Elnemr</span>
                        </h1>
                        <table class="table table-borderd">
                            <thead>
                                <tr>
                                    <td class="text-success">Request Name</td>
                                    <td class="text-success">Start Date</td>
                                </tr>
                            </thead>
                            <tbody>
                                <tr>
                                    <td><t t-esc="doc.name"></t></td>
                                    <td><t t-esc="doc.date_from"></t></td>
                                </tr>
                            </tbody>
                        </table>
                    </t>
                </t>
            </t>
        </template>
    </data>
</odoo>