<odoo>
    <data>
        <template id="car_request" name="Car Request">
            <t t-call="website.layout">
                <div class="oe_structure"/>
                <div class="container">
                    <h1 class="text-danger">Hello World</h1>
                    <table class="table table-hover">
                        <thead>
                            <tr>
                                <th>Car Request Name</th>
                                <th>Starting Date</th>
                                <th>State</th>
                            </tr>
                        </thead>
                        <tbody>
                            <t t-foreach="objects" t-as="obj">
                                <tr>
                                    <td class="text-warning">
                                        <t t-esc="obj.name"/>
                                    </td>
                                    <td>
                                        <t t-esc="obj.date_from"/>
                                    </td>
                                    <td t-att-class="obj.state=='draft' and 'text-secondary' or obj.state=='confirm' and 'text-success' or  obj.state=='refuse' and 'text-danger' ">
                                         <t t-if="obj.state == 'draft'">
                                             Draft
                                         </t>
                                         <t t-if="obj.state == 'confirm'">
                                             Confirm
                                         </t>
                                         <t t-if="obj.state == 'validate'">
                                             Validate
                                         </t>
                                         <t t-if="obj.state == 'refuse'">
                                             Refused
                                         </t>

                                    </td>
                                </tr>
                            </t>
                        </tbody>
                    </table>
                </div>
                <div class="oe_structure"/>
            </t>
        </template>
    </data>
</odoo>